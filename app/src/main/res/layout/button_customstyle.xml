<?xml version="1.0" encoding="utf-8"?>
<Button xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_vertical|center_horizontal"
    android:textColor="@color/button_text_color"
    android:shadowColor="@color/button_shadow_color"
    android:shadowDx="@integer/button_shadow_dx"
    android:shadowDy="@integer/button_shadow_dy"
    android:shadowRadius="@integer/button_shadow_radius"
    android:textSize="16dip"
    android:textStyle="normal"
    android:background="@drawable/button_customstyle"
    android:focusable="true"
    android:clickable="true"/>
